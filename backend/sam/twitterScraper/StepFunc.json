{
  "Start At": "scrape-All",
  "States": {
    "scrape-All": {
      "Type": "Map",
      "ItemsPath": "$.scrape-detail",
      "MaxConcurrency": 0,
      "Parameters": {
        "content.$": "$$.Map.Item.Value",
        "scrape-until": "$.scrape-until"
      },
      "Iterator": {
        "StartAt": "scrapet",
        "States": {
          "scrapet": {
            "Type": "Task",
            "Resource": "arn:aws:lambda:eu-west-1:878292117449:function:Twitter_scraper",
            "End": true
          }
        }
      },
      "next": "getSentiment"
    },
    "getSentiment" : {
      "Type": "Map",
      "Next": "Finish",
      "MaxConcurrency": 0,
      "Iterator": {
        "StartAt": "SentimentMap",
         "States": {
           "SentimentMap" :{
             "Type": "Task",
             "Resource": "arn:aws:lambda:eu-west-1:878292117449:function:Sentiment_Analysis",
             "End": true
           }
         }
      }
    },
    "Finish": {
      "Type": "Succeed",
      "End" : "True"
    }
  }
}


